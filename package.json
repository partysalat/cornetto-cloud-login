{
  "name": "cornetto-login",
  "version": "1.0.0",
  "description": "Cornetto Cloud login",
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/partysalat/cornetto-cloud-login"
  },
  "scripts": {
    "postinstall": "sls dynamodb install",
    "clean": "rm -rf target",
    "clean:light": "rm -rf target/assets",
    "test": "npm run unit",
    "server": "gulp serve & npm run dynamodb:start & sls offline start",
    "dynamodb:start": "sls dynamodb start --migrate",
    "watch": "npm run clean:light && (npm run server & gulp watch & npm run unit:watch)",
    "start": "npm run clean && npm run browser:build && npm run server",
    "deploy": "npm run build && (npm run browser:deploy & npm run server:deploy)",
    "server:deploy": "sls deploy -v --stage pro",
    "browser:deploy": "gulp deploy:assets",
    "browser:build": "gulp build:client",
    "unit:server": "ava lib/server/**/*.spec.* --source 'lib/server/**/*!(spec).js' --require babel-register",
    "unit:browser": "ava lib/browser/**/*.spec.js --source 'lib/browser/**/*!(spec).js' --require ./test/browser/setup-browser-env.js --require babel-register --require ignore-styles",
    "unit:server:watch": "npm run unit:server -- --watch",
    "unit:browser:watch": "npm run unit:browser -- --watch",
    "unit": "npm run unit:server && npm run unit:browser",
    "unit:watch": "npm run unit:server:watch & npm run unit:browser:watch",
    "build": "npm run clean && npm run browser:build && npm run lint && npm run unit",
    "lint": "eslint --ext .js --ext .jsx lib/",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Ben Lenser",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.16.4",
    "prop-types": "^15.5.10",
    "react-router-dom": "^4.2.2",
    "aws-sdk": "^2.142.0"
  },
  "devDependencies": {
    "amazon-cognito-auth-js": "^1.0.0",
    "amazon-cognito-identity-js": "^1.21.0",
    "ava": "^0.16.0",
    "ava-spec": "^1.0.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "bootstrap-styl": "^5.0.8",
    "browser-env": "^2.0.12",
    "browser-sync": "^2.17.2",
    "css-loader": "^0.25.0",
    "d3": "^4.2.8",
    "enzyme": "^2.5.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.14.1",
    "gulp-awspublish": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-serve": "^1.4.0",
    "ignore-styles": "^5.0.1",
    "jeet": "^6.1.5",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.3.2",
    "react-hammerjs": "^1.0.1",
    "require-dir": "^0.3.1",
    "serverless": "^1.22.0",
    "serverless-dynamodb-local": "^0.2.26",
    "serverless-offline": "3.16.0",
    "serverless-webpack": "3.1.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-stream": "^3.2.0"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "tap": false,
    "powerAssert": false,
    "babel": "inherit",
    "verbose": true
  }
}
